{
  "name": "kadernik-plus-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npm run sentry:sourcemaps",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "seed": "ts-node -P tsconfig.json -r tsconfig-paths/register prisma/seed.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yaml}\" --ignore-path ./.prettierignore",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org kadernik --project node ./dist && sentry-cli sourcemaps upload --org kadernik --project node ./dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@sentry/cli": "^2.53.0",
    "@sentry/node": "^10.19.0",
    "@sentry/node-native": "^10.11.0",
    "@sentry/profiling-node": "^10.19.0",
    "@sentry/tracing": "^7.120.4",
    "@types/axios": "^0.14.4",
    "@types/lodash": "^4.17.20",
    "auth0": "^4.27.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-prom-bundle": "^8.0.0",
    "express-rate-limit": "^8.1.0",
    "mysql2": "^3.15.1",
    "node-cron": "^4.2.1",
    "prom-client": "^15.1.3",
    "prometheus": "^0.1.7",
    "radash": "^12.1.1",
    "react-lodash": "^0.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "ts-node -P tsconfig.json -r tsconfig-paths/register prisma/seed.ts"
  }
}
